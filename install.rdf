<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>duckduckbang@luk3yx.github.io</em:id>
    <em:name>DuckDuckBang!</em:name>
    <em:description>Allows you to use DuckDuckGo !bangs on Google, Yahoo!, and Bing. (Pale Moon version)</em:description>
    <em:version>0.1-2</em:version>
    <em:creator>luk3yx</em:creator>
    <em:iconURL>chrome://duckduckbang!/content/icon.png</em:iconURL>
    <em:homepageURL>https://github.com/luk3yx/duckduckbang</em:homepageURL>
    <em:type>2</em:type>

    <!-- Mozilla Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>4.0</em:minVersion>
        <em:maxVersion>47.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Pale Moon -->
    <em:targetApplication>
      <Description>
        <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
        <em:minVersion>27.0.0</em:minVersion>
        <em:maxVersion>27.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
